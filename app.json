{
  "expo": {
    "name": "Reminders App",
    "slug": "reminders-app",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./appIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#F9F8F6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.justinmoran.remindersapp",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F9F8F6"
      },
      "package": "com.justinmoran.remindersapp",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#D4AF37",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e80d3dc8-cc84-4860-9e6e-b42447721fce"
      }
    }
  }
}
